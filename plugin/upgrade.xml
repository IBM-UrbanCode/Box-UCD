<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin-upgrade xmlns="http://www.urbancode.com/UpgradeXMLSchema_v1" xmlns:server="http://www.urbancode.com/PluginServerXMLSchema_v1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!-- The purpose of plugin.xml is to provide upgrade paths when the plugin version is changed.
       If the upgrade paths are specified correctly, users will be upgraded to the new version 
       without interruption. -->
	<migrate to-version="2">
		<migrate-command name="Request Authentication"/>
	</migrate>
	<migrate to-version="3">
		<migrate-command name="Request Authentication"/>
	</migrate>
	<migrate to-version="5">
		<migrate-command name="Authenticate and Upgrade APP"/>
	</migrate>
	<migrate to-version="6">
		<migrate-command name="Authenticate and Upgrade APP"/>
		<migrate-command name="Upgrade APP"/>
	</migrate>
	<migrate to-version="7">
		<migrate-command name="Box Request Authentication"/>
		<migrate-command name="Box Upload File"/>
	</migrate>
	<migrate to-version="26">
		<migrate-command name="Box Request Enterprise Authentication"/>
		<migrate-command name="Box Create App User"/>
		<migrate-command name="Box Request App User Authentication"/>
		<migrate-command name="Box Create Folder"/>
		<migrate-command name="Box Upload File"/>
		<migrate-command name="Box Get File Download Url"/>
	</migrate>
</plugin-upgrade>
